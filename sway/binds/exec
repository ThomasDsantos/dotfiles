bindsym {
  $mod+Return       exec $term
  $mod+Shift+q      kill
  $mod+Shift+r      reload

  $mod+p            exec killall wofi || cliphist list | wofi -S dmenu | cliphist decode | wl-copy
  $mod+d            exec killall wofi || wofi --show run
  $mod+Shift+d      exec killall wofi || wofi --show drun
  $mod+Shift+e      exec killall wofi || wofi-emoji
}

