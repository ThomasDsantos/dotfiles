bindsym {
  $mod+Return   exec $term
  $mod+Shift+q  kill
  $mod+d        exec killall wofi || wofi --show run
  $mod+Shift+d  exec killall wofi || wofi --show drun
  $mod+Shift+r  reload
  $mod+Shift+e  exec nautilus -w
  $mod+p        exec cliphist list | wofi -S dmenu | cliphist decode | wl-copy
}

