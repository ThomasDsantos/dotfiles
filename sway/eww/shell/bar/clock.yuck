(defwidget clock [screen]
    (box :space-evenly false
        (button 
            :onclick "bin/popup.sh settings-panel settings-panel-shown open ${screen}"
            :timeout "16s"
            :class "button"
            "${weather-text-icons[(weather?.weather?.[0]?.icon ?: "01d")]}    ${formattime(EWW_TIME, "%m/%d - %H:%M")}")))
