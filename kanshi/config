profile home {
  output "eDP-1" mode 1920x1200 position 0,0
  output "Samsung Electric Company LC49G95T H4ZR701011" mode 5120x1440 position 1920,0 scale 1
  exec swaymsg workspace 1, move workspace to eDP-1
  exec swaymsg workspace 2, move workspace to output '"Samsung Electric Company LC49G95T H4ZR701011"'
  exec swaymsg workspace 3, move workspace to output '"Samsung Electric Company LC49G95T H4ZR701011"'
  exec swaymsg workspace 4, move workspace to output '"Samsung Electric Company LC49G95T H4ZR701011"'
  exec swaymsg workspace 5, move workspace to output '"Samsung Electric Company LC49G95T H4ZR701011"'
  exec swaymsg workspace 6, move workspace to output '"Samsung Electric Company LC49G95T H4ZR701011"'
  exec swaymsg workspace 7, move workspace to output '"Samsung Electric Company LC49G95T H4ZR701011"'
  exec swaymsg workspace 8, move workspace to output '"Samsung Electric Company LC49G95T H4ZR701011"'
  exec swaymsg workspace 9, move workspace to output '"Samsung Electric Company LC49G95T H4ZR701011"'
  exec swaymsg workspace 10, move workspace to output '"Samsung Electric Company LC49G95T H4ZR701011"'
  exec eww-setup
}

